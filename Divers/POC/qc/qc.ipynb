   "source": [
    "from qiskit import *\n",
    "\n",
    "qr = QuantumRegister(2, \"qr\")\n",
    "cr = ClassicalRegister(2, \"cr\")\n",
    "\n",
    "qc = QuantumCircuit(qr, cr)\n",
    "\n",
    "qc.h(qr[0])\n",
    "qc.cx(qr[0], qr[1])\n",
    "\n",
    "qc.measure(qr, cr)\n",
    "\n",
    "qc.draw(output='latex_source', filename='./qc-draw.tex')\n",
    "qc.draw()"
   ]
  }
